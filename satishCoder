#include<stdio.h>
int main()
{
int i,j,sum=0,x[3][3];
printf("enter element of array..\n");
for(i=0;i<3;i++)
{
    for(j=0;j<3;j++)
    {
        printf("x[%d][%d]=",i,j);
        scanf("%d",&x[i][j]);
    }
}
printf("Array elements..\n");
for(i=0;i<3;i++)
{
    for(j=0;j<3;j++)
    {
        printf("%d\t",x[i][j]);
    }
    printf("\n");
}
printf("\n");

for(i=0;i<3;i++)
{
    for(j=0;j<3;j++)
    {
        if(i==j)
        {
            sum=sum+x[i][j];
        }

    }
    
}
printf("the sum of diagonal=%d",sum);10
return 0;
}
